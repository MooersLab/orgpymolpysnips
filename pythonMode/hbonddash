# name: Set up H-bond dashes.
# group: pymolpy-H-bonds
# key: hbonddash
# --
cmd.do("hide everything, hydrogens;")
cmd.do("hide labels;")
cmd.do("set dash_color, black; ")
cmd.do("set dash_gap, 0.4;")
cmd.do("set dash_radius, 0.08;")
$0
